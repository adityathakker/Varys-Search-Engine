        <input type="text" ng-model="searchQuery" placeholder="Enter Search Term" id="search-input">
        <div data-search-autocomplete class="search-autocomplete">
          <ul>
            <div class="header" >
              Categories
            </div>
            <li ng-repeat="suggestion in suggestedTaxonomies track by $index" class="suggestion">
              <a
                ng-click="autocompleteClick(suggestion)"
                ng-href="{{suggestion.url}}">
                {{suggestion.name}}
              </a>
            </li>

            <div class="header">
              Profiles
            </div>
            <li ng-repeat="suggestion in suggestedUsers track by $index" class="suggestion">
              <a
                ng-click="autocompleteClick(suggestion)"
                ng-href="{{suggestion.url}}">
                {{suggestion.name}}
              </a>
            </li>
          </ul>
        </div>
      </form>

      <!-- DISCOVER -->
      <div class="discover deck-3" ng-cloak ng-hide="hideDiscover">
        <div class="main-column">
          <div class="card" ng-repeat="discover in discoverList">
            <a href="{{discover.url}}" ng-click="trackDiscoverLink(discover)">
              <img ng-src="{{discover.imgUrl}}" class="img-large">
              <img ng-src="{{discover.imgUrlSmall}}" class="img-small">
              <div class="title-wrapper">
                <div class="title">{{discover.title}}</div>
              </div>
            </a>
          </div>
        </div>
      </div>

      <div class="search-status">
        <div ng-show="isLoading">
            <img alt="Search Loading" class="loader-image"/>
        </div>
        <div class="search-message">{{searchMessage}}</div>
      </div>

      <!-- SEARCH RESULTS -->
      <div class="search-result article-feed">
        <div class="article" ng-repeat="article in articles">
          <div class="article-image">
            <a href='{{article.articlePath}}'>
              <img ng-src='{{article.imagePath}}' />
            </a>
          </div>
          <div class="article-title"><a href='{{article.articlePath}}'>{{article.title}}</a></div>
          <div class="article-author" ng-if="article.byline === ''">{{article.author}}</div>
          <div class="article-author" ng-if="article.byline !== ''">{{article.byline}}</div>
        </div>
      </div>
    </div>
  </div>
</div>

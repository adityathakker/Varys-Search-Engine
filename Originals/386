	function iEncodePixelUrl( id, event, data, key )
	{
		var endpoint = "anthill.instapage.com" + '/projects/' + id + '/events/' + event;
		var query = 'api_key=' + key + '&data=' + base64_encode( JSON.stringify( data ) );
		var image_url = '//' + endpoint + '?' + query;

		return image_url;
	}


  data-media="http%3A%2F%2Fgreatist.com%2Fsites%2Fdefault%2Ffiles%2Fstyles%2Fbig_share%2Fpublic%2FPinterestBanner_32_Healthier_Tacos_0.jpg%3Fitok%3DG4VFoEQH"
  data-mediatitle="32 Healthier Ways to Eat Tacos Every Day"
  data-title="32 Healthier Ways to Eat Tacos Every Day"
  data-promotitle="32 Healthier Ways to Eat Tacos Every Day"
  data-pintitle="Turn every night into Taco Tuesday."
  data-twitter="https://twitter.com/jhavanis"
  data-twitter-url="http://grt.st/1Fx07XK"
  class="share-buttons"
  data-origin="side"

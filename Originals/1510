      itemtype="http://schema.org/Article"
    >
      <div class="article-top">
                  <div itemprop="description" class="element-hidden">Two minutes is it all it takes—really.</div>
                <header class="article-header">
          <div class="group-article-meta">
            <div
  class="field field-name-post-date"
  itemprop="datePublished"
  content="2016-06-08"
>
  June 8, 2016  <span class="separator">|</span> </div>
            <div class="field article-author-top" itemscope itemprop="author" itemtype="http://schema.org/Person">
  <span class="label">By </span>
       <span><a
          href="http://greatist.com/p/jamie-varon"
          title="User profile of "
          class="username"
          about="http://greatist.com/p/jamie-varon"
          typeof="sioc:UserAccount"
          itemprop="url"
        ><span itemprop="name">Jamie Varon</span>
        </a>
      </span>
    </div>
          </div>
          <h1 class="title" id="page-title" itemprop="name headline alternativeHeadline">How I Finally Quit Making Excuses and Got Healthy Habits to Stick</h1>
        </header>
        <div class="top-share">
          <div class="group_share" data-recirc-section>
  <div class="share-buttons icons" data-origin="top">
    <div ng-controller="socialCtrl"
  data-href="http://greatist.com/live/healthy-habits-tips-that-finally-made-habits-stick"

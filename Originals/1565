  data-mediatitle="13 Unexpected Benefits of Exercise"
  data-title="13 Unexpected Benefits of Exercise"
  data-promotitle="13 Unexpected Benefits of Exercise"
  data-pintitle="Whether you’re looking to rev up brainpower, calm racing thoughts, or boost overall happiness..."
  data-twitter=""
  data-twitter-url="http://grt.st/10Go1g3"
  class="share-buttons"
  data-origin="side"
>
      <a class="share-pinterest" ng-click="share_pin()">
      <i class="fa fa-pinterest-p"></i>
    </a>
        <a class="share-facebook" ng-click="share_fb($event)" data-share-fb>
      <i class="fa fa-facebook"></i>
    </a>
    <a class="share-twitter" ng-click="share_tw()">
    <i class="fa fa-twitter"></i>

      itemscope
      itemtype="http://schema.org/Article"
    >
      <div class="article-top">
                  <div itemprop="description" class="element-hidden">No matter your career goals, creating an online presence is incredibly important. Here&#039;s why.</div>
                <header class="article-header">
          <div class="group-article-meta">
            <div
  class="field field-name-post-date"
  itemprop="datePublished"
  content="2016-05-03"
>
  May 3, 2016  <span class="separator">|</span> </div>
            <div class="field article-author-top" itemscope itemprop="author" itemtype="http://schema.org/Person">
  <span class="label">By </span>
       <span><a
          href="http://greatist.com/p/susie-moore"
          title="User profile of "
          class="username"
          about="http://greatist.com/p/susie-moore"
          typeof="sioc:UserAccount"
          itemprop="url"
        ><span itemprop="name">Susie Moore</span>
        </a>
      </span>
    </div>
          </div>
          <h1 class="title" id="page-title" itemprop="name headline alternativeHeadline">How to Build a Personal Brand Without Being Obnoxious</h1>
        </header>
        <div class="top-share">
          <div class="group_share" data-recirc-section>
  <div class="share-buttons icons" data-origin="top">
    <div ng-controller="socialCtrl"

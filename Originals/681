  data-media="http%3A%2F%2Fgreatist.com%2Fsites%2Fdefault%2Ffiles%2Fstyles%2Fbig_share%2Fpublic%2FMealsForOne_Banner.jpg%3Fitok%3D-iP17uS9"
  data-mediatitle="Cooking for One: 29 Insanely Easy, Healthy Meals You Can Make in Minutes"
  data-title="Cooking for One: 29 Insanely Easy, Healthy Meals You Can Make in Minutes"
  data-promotitle="29 Insanely Easy, Healthy Meals for One"
  data-pintitle="One is no longer the loneliest number in the kitchen. These creative, easy-peasy recipes will..."
  data-twitter=""

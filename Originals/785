    ng-controller="ArticleCtrl"
    ng-init="init()">
  <div class="article-main">
    <article
      class="article-main-column main-column"
      itemscope
      itemtype="http://schema.org/Article"
    >
      <div class="article-top">
                  <div itemprop="description" class="element-hidden">Consider this your cheat sheet to mastering the common poses you&#039;re likely to flow through in vinyasa class.</div>
                <header class="article-header">
          <div class="group-article-meta">
            <div
  class="field field-name-post-date"
  itemprop="datePublished"
  content="2015-03-10"
>
  March 10, 2015  <span class="separator">|</span> </div>
            <div class="field article-author-top" itemscope itemprop="author" itemtype="http://schema.org/Person">
  <span class="label">By </span>
    <a
    href="http://greatist.com/p/erin-kelly"
    title="User profile of "
    class="username"
    about="http://greatist.com/p/erin-kelly"
    typeof="sioc:UserAccount"
    itemprop="url"
  >
    <span itemprop="name">Erin Kelly</span>
  </a>
   </div>
          </div>
          <h1 class="title" id="page-title" itemprop="name headline alternativeHeadline">The Only 30 Yoga Poses You Really Need to Know</h1>
        </header>
        <div class="top-share">
          <div class="group_share" data-recirc-section>
  <div class="share-buttons icons" data-origin="top">
    <div ng-controller="socialCtrl"
  data-href="http://greatist.com/move/common-yoga-poses"
  data-media="http%3A%2F%2Fgreatist.com%2Fsites%2Fdefault%2Ffiles%2Fstyles%2Fbig_share%2Fpublic%2FYogaPoses_Pinterest.jpg%3Fitok%3DkTNiglud"
  data-mediatitle="The Only 30 Yoga Poses You Really Need to Know"
  data-title="The Only 30 Yoga Poses You Really Need to Know"
  data-promotitle="The Only 30 Yoga Poses You Need to Know"
  data-pintitle="Consider this your cheat sheet to mastering the common poses you\'re likely to flow through in..."
  data-twitter="http://twitter.com/erinkellysays"
  data-twitter-url="http://grt.st/1851lgV"
  class="share-buttons"
>
  <div class="share-line share-line-first">

  data-media="http%3A%2F%2Fgreatist.com%2Fsites%2Fdefault%2Ffiles%2Fstyles%2Fbig_share%2Fpublic%2FLunge_ExtendedPinterestBanner_0.png%3Fitok%3Dpt36yEjp"
  data-mediatitle="32 Lunge Variations to Keep Things Interesting (and Seriously Intense)"
  data-title="32 Lunge Variations to Keep Things Interesting (and Seriously Intense)"
  data-promotitle="32 Lunge Variations to Keep Things Interesting (and Seriously Intense)"
  data-pintitle="There\'s way more to the lunge than the simple bodyweight version we all know and love (to hate)..."

      class="article-main-column main-column"
      itemscope
      itemtype="http://schema.org/Article"
    >
      <div class="article-top">
                  <div itemprop="description" class="element-hidden">Turns out, your fave suspension trainer can support and advance your yoga practice.</div>
                <header class="article-header">
          <div class="group-article-meta">
            <div
  class="field field-name-post-date"
  itemprop="datePublished"
  content="2016-06-06"
>
  June 6, 2016  <span class="separator">|</span> </div>
            <div class="field article-author-top" itemscope itemprop="author" itemtype="http://schema.org/Person">
  <span class="label">By </span>
       <span><a
          href="http://greatist.com/p/mollyritterbeck"
          title="User profile of "
          class="username"
          about="http://greatist.com/p/mollyritterbeck"
          typeof="sioc:UserAccount"
          itemprop="url"
        ><span itemprop="name">molly ritterbeck</span>
        </a>
      </span>
    </div>
          </div>
          <h1 class="title" id="page-title" itemprop="name headline alternativeHeadline">11 Surprising Ways to Use TRX for Yoga </h1>
        </header>
        <div class="top-share">
          <div class="group_share" data-recirc-section>
  <div class="share-buttons icons" data-origin="top">
    <div ng-controller="socialCtrl"
  data-href="http://greatist.com/move/trx-yoga-workout"
  data-media="http%3A%2F%2Fgreatist.com%2Fsites%2Fdefault%2Ffiles%2Fstyles%2Fbig_share%2Fpublic%2FPinterestBanner_TRX_Yoga_Crow.jpg%3Fitok%3DPe_5IWdf"
  data-mediatitle="11 Surprising Ways to Use TRX for Yoga "

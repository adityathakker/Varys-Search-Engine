  data-media="http%3A%2F%2Fgreatist.com%2Fsites%2Fdefault%2Ffiles%2Fstyles%2Fbig_share%2Fpublic%2FPinterestBanner_TRX_Yoga_Crow.jpg%3Fitok%3DPe_5IWdf"
  data-mediatitle="11 Surprising Ways to Use TRX for Yoga "
  data-title="11 Surprising Ways to Use TRX for Yoga "
  data-promotitle="11 Surprising Ways to Use TRX for Yoga "
  data-pintitle="Turns out, your fave suspension trainer can support and advance your yoga practice."
  data-twitter="https://twitter.com/MollyRitt"
  data-twitter-url="http://grt.st/1XMoNFP"
  class="share-buttons"
  data-origin="side"

  "@context": "http://schema.org",
  "@type": "Article",
  "mainEntityOfPage": {
    "@type":"WebPage",
    "@id":"http://greatist.com/happiness/20-bad-habits-could-hurt-your-relationship"
  },
  "headline": "20 Bad Habits That Could Hurt Your Relationship",
  
  "image": {
    "@type": "ImageObject",
    "url": "http://greatist.com/sites/default/files/850.jpg",
    "height": 1236,
    "width": 1800
  },
  "datePublished": "2013-08-14T13:28:00-04:00",
  "dateModified": "2016-07-28T13:13:06-04:00",
  "description": "Want to lose the love of your life? Didn’t think so. Avoid these bad habits that could harm a happy relationship.
",
  "about": [
    {
      "@type": "Thing",
      "name": "Play",
      "sameAs": "http://greatist.com/play"
    },
    {
      "@type": "Thing",

  "mainEntityOfPage": {
    "@type":"WebPage",
    "@id":"http://greatist.com/move/exercise-tips-that-anyone-can-follow"
  },
  "headline": "The Expert Exercise Tips That Anyone Can Follow",
  
  "image": {
    "@type": "ImageObject",

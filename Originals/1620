        <input type="submit" value="GO" ng-hide="submitted">
        <span class="success" ng-show="submitted" ng-cloak><i class="fa fa-check"></i></span>
      </div>
      <div class="newsletter-messages">
        <div class="message message-error" ng-show="error" ng-cloak>{{error}}</div>
      </div>
    </form>
  </div>


      <a class="share-pinterest" ng-click="share_pin()">
      <i class="fa fa-pinterest-p"></i>
    </a>
        <a class="share-facebook" ng-click="share_fb($event)" data-share-fb>
      <i class="fa fa-facebook"></i>
    </a>
    <a class="share-twitter" ng-click="share_tw()">
    <i class="fa fa-twitter"></i>

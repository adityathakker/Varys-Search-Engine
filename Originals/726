  "@type": "Article",
  "mainEntityOfPage": {
    "@type":"WebPage",
    "@id":"http://greatist.com/health/30-dairy-free-recipe-substitutions"
  },
  "headline": "30 Dairy-Free Recipe Substitutions ",
  
  "image": {
    "@type": "ImageObject",
    "url": "http://greatist.com/sites/default/files/wp-content/uploads/2011/12/Milk-Cookies_large_0.jpg",
    "height": 467,
    "width": 900
  },
  "datePublished": "2011-12-15T11:18:56-05:00",
  "dateModified": "2016-06-06T15:59:28-04:00",
  "description": "<p>Is dairy off limits? No problem! </p>
",
  "about": [
    {
      "@type": "Thing",
      "name": "Health",
      "sameAs": "http://greatist.com/health"
    },
    {
      "@type": "Thing",
      "name": "Baking",
      "sameAs": "http://greatist.com/baking"
    },
    {
      "@type": "Thing",
      "name": "Food Swaps",
      "sameAs": "http://greatist.com/food-swaps"

      itemscope
      itemtype="http://schema.org/Article"
    >
      <div class="article-top">
                  <div itemprop="description" class="element-hidden">It doesn&#039;t have to be the bane of your existence.</div>
                <header class="article-header">
          <div class="group-article-meta">
            <div
  class="field field-name-post-date"
  itemprop="datePublished"
  content="2016-04-26"
>
  April 26, 2016  <span class="separator">|</span> </div>
            <div class="field article-author-top" itemscope itemprop="author" itemtype="http://schema.org/Person">
  <span class="label">By </span>
    <a
    href="http://greatist.com/p/sophia-breene"
    title="User profile of "
    class="username"
    about="http://greatist.com/p/sophia-breene"
    typeof="sioc:UserAccount"
    itemprop="url"
  >
    <span itemprop="name">Sophia Breene</span>
  </a>
   </div>
  <div class="author-infos">
    - Additional research by Amy Eisinger  </div>
          </div>
          <h1 class="title" id="page-title" itemprop="name headline alternativeHeadline">How to Make Your Commute Suck Way Less</h1>
        </header>
        <div class="top-share">
          <div class="group_share" data-recirc-section>
  <div class="share-buttons icons" data-origin="top">

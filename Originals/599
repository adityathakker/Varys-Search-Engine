  data-media="http%3A%2F%2Fgreatist.com%2Fsites%2Fdefault%2Ffiles%2Fstyles%2Fbig_share%2Fpublic%2FGreatist_Banner_34_Healthy-Breakfasts-2_0.jpg%3Fitok%3D-rPRiJBJ"
  data-mediatitle="34 Healthy Breakfasts for Busy Mornings"
  data-title="34 Healthy Breakfasts for Busy Mornings"
  data-promotitle="34 Healthy Breakfasts for Busy Mornings"
  data-pintitle="Many of these healthy breakfast ideas are perfect for packing as snacks too!"

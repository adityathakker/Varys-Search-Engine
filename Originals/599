      itemscope
      itemtype="http://schema.org/Article"
    >
      <div class="article-top">
                  <div itemprop="description" class="element-hidden">Many of these healthy breakfast ideas are perfect for packing as snacks too! </div>
                <header class="article-header">
          <div class="group-article-meta">
            <div
  class="field field-name-post-date"
  itemprop="datePublished"
  content="2016-03-30"
>
  March 30, 2016  <span class="separator">|</span> </div>
            <div class="field article-author-top" itemscope itemprop="author" itemtype="http://schema.org/Person">
  <span class="label">By </span>
    <a
    href="http://greatist.com/p/kate-morin"
    title="User profile of "
    class="username"
    about="http://greatist.com/p/kate-morin"
    typeof="sioc:UserAccount"
    itemprop="url"
  >
    <span itemprop="name">Kate Morin</span>
  </a>
   </div>
          </div>
          <h1 class="title" id="page-title" itemprop="name headline alternativeHeadline">34 Healthy Breakfasts for Busy Mornings</h1>
        </header>
        <div class="top-share">
          <div class="group_share" data-recirc-section>
  <div class="share-buttons icons" data-origin="top">
    <div ng-controller="socialCtrl"
  data-href="http://greatist.com/health/healthy-fast-breakfast-recipes"
  data-media="http%3A%2F%2Fgreatist.com%2Fsites%2Fdefault%2Ffiles%2Fstyles%2Fbig_share%2Fpublic%2FGreatist_Banner_34_Healthy-Breakfasts-2_0.jpg%3Fitok%3D-rPRiJBJ"

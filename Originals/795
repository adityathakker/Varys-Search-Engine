    ng-controller="ArticleCtrl"
    ng-init="init()">
  <div class="article-main">
    <article
      class="article-main-column main-column"
      itemscope
      itemtype="http://schema.org/Article"
    >
      <div class="article-top">
                  <div itemprop="description" class="element-hidden">Improve your power and agility and add intensity to any workout with these moves.</div>
                <header class="article-header">
          <div class="group-article-meta">
            <div
  class="field field-name-post-date"
  itemprop="datePublished"
  content="2016-01-05"
>
  January 5, 2016  <span class="separator">|</span> </div>
            <div class="field article-author-top" itemscope itemprop="author" itemtype="http://schema.org/Person">
  <span class="label">By </span>
       <span><a
          href="http://greatist.com/p/greatist-team"
          title="User profile of "
          class="username"
          about="http://greatist.com/p/greatist-team"
          typeof="sioc:UserAccount"
          itemprop="url"
        ><span itemprop="name">The Greatist Team</span>
        </a>
      </span>
    </div>
          </div>
          <h1 class="title" id="page-title" itemprop="name headline alternativeHeadline">19 Powerful Bodyweight Exercises for Strength and Speed</h1>
        </header>
        <div class="top-share">
          <div class="group_share" data-recirc-section>
  <div class="share-buttons icons" data-origin="top">
    <div ng-controller="socialCtrl"
  data-href="http://greatist.com/fitness/explosive-bodyweight-exercises"
  data-media="http%3A%2F%2Fgreatist.com%2Fsites%2Fdefault%2Ffiles%2Fstyles%2Fbig_share%2Fpublic%2FGreatist_Banner_Bodyweight_moves.jpg%3Fitok%3DxOrFuiq_"
  data-mediatitle="19 Powerful Bodyweight Exercises for Strength and Speed"
  data-title="19 Powerful Bodyweight Exercises for Strength and Speed"
  data-promotitle="19 Powerful Bodyweight Exercises for Strength and Speed"
  data-pintitle="Improve your power and agility and add intensity to any workout with these moves."
  data-twitter="http://twitter.com/greenfit_health"
  data-twitter-url="http://grt.st/177adSh"
  class="share-buttons"
>
  <div class="share-line share-line-first">

<meta name="sailthru.title" content="12 Things You Don&#039;t Actually Need to Have Figured Out by Age 30" />
<link href="http://greatist.com/live/things-you-dont-need-to-have-figured-out-by-age-30" rel="canonical" />
<script type="application/ld+json">
  {
  "@context": "http://schema.org",
  "@type": "Article",
  "mainEntityOfPage": {
    "@type":"WebPage",
    "@id":"http://greatist.com/live/things-you-dont-need-to-have-figured-out-by-age-30"
  },
  "headline": "12 Things You Don't Actually Need to Have Figured Out by Age 30",
  
  "image": {
    "@type": "ImageObject",
    "url": "http://greatist.com/sites/default/files/676.jpeg",
    "height": 3567,
    "width": 5350
  },
  "datePublished": "2016-04-18T05:00:00-04:00",
  "dateModified": "2016-07-15T16:31:11-04:00",
  "description": "Let's all take a big, deep breath. ",
  "about": [
    {
      "@type": "Thing",
      "name": "Live",
      "sameAs": "http://greatist.com/live"
    },
    {
      "@type": "Thing",
      "name": "Overshare",
      "sameAs": "http://greatist.com/overshare"

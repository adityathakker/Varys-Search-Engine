<meta name="sailthru.title" content="How to Stop Any Craving Right When It Hits" />
<meta name="sailthru.date" content="2016-06-06T04:00:00-04:00" />
<link href="http://greatist.com/eat/how-to-stop-cravings-fast" rel="canonical" />
<script type="application/ld+json">
  {
  "@context": "http://schema.org",
  "@type": "Article",
  "mainEntityOfPage": {
    "@type":"WebPage",
    "@id":"http://greatist.com/eat/how-to-stop-cravings-fast"
  },
  "headline": "How to Stop Any Craving Right When It Hits",
  
  "image": {
    "@type": "ImageObject",
    "url": "http://greatist.com/sites/default/files/cravings-feature.jpg",
    "height": 1236,
    "width": 1800
  },
  "datePublished": "2016-06-06T04:00:00-04:00",
  "dateModified": "2016-06-06T14:26:26-04:00",
  "description": " Use these expert tips to silence your inner cookie monster.",
  "about": [
    {
      "@type": "Thing",
      "name": "Eat",
      "sameAs": "http://greatist.com/eat"
    },
    {
      "@type": "Thing",
